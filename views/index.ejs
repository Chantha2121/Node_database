<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User List</title>
</head>
<body>
    <% if (r && result.array && result.array.length > 0) { %>
        <% result.array.forEach(element => { %>
            <h1>Name : <%= element.name %></h1>
            <h1>ID : <%= element.id %></h1>
        <% }); %>
    <% } else { %>
        <p>No data available</p>
    <% } %>
</body>
</html>
